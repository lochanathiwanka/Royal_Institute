            <TableColumn fx:id="clmAddress" prefWidth="111.0" text="Address" />
        </columns>
         <effect>
            <DropShadow />
         </effect>
      </TableView>
      <JFXTextField labelFloat="true" layoutX="436.0" layoutY="111.0" onKeyReleased="#txtFindOnActionOnKeyReleased" promptText="Find Student">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <TextField fx:id="txtID" editable="false" layoutX="507.0" layoutY="198.0" style="-fx-background-radius: 10;" />
