      </JFXButton>
      <JFXButton fx:id="btnAdd" buttonType="RAISED" layoutX="451.0" layoutY="351.0" onAction="#btnAddOnAction" prefHeight="26.0" prefWidth="65.0" style="-fx-background-color: #647C90;" text="Add" textFill="WHITE">
