      </JFXButton>
      <JFXButton fx:id="btnDelete" buttonType="RAISED" layoutX="613.0" layoutY="351.0" onAction="#btnDeleteOnAction" prefHeight="26.0" prefWidth="65.0" style="-fx-background-color: #647C90;" text="Delete" textFill="WHITE">
